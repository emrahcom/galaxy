#!/bin/bash
set -e

BASEDIR=$(dirname $0)
TEMPLATES=$BASEDIR/../../templates
source $BASEDIR/../../../config
source $BASEDIR/../../common

# ------------------------------------------------------------------------------
# get public meeting list (limit 2, offset 2)
# ------------------------------------------------------------------------------
JSON=$(cat <<EOF
{
    "limit": 2,
    "offset": 2
}
EOF
)

bash $TEMPLATES/action-guest-attr "pub" "meeting" "list" "id" "$JSON" 3
