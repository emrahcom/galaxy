#!/bin/bash
set -e

BASEDIR=$(dirname $0)
TEMPLATES=$BASEDIR/../../templates
source $BASEDIR/../../common

# ------------------------------------------------------------------------------
# set as default a nonexistent profile
# ------------------------------------------------------------------------------
profile_fake=$(uuidgen)
JSON=$(cat <<EOF
{
    "id": "$profile_fake"
}
EOF
)

bash $TEMPLATES/action-member-empty "pri" "profile" "set/default" "$JSON" 1
