#!/bin/bash
set -e

BASEDIR=$(dirname $0)
TEMPLATES=$BASEDIR/../../templates
source $BASEDIR/../../common

[[ -z "$1" ]] && SET=1 || SET=$1

# ------------------------------------------------------------------------------
# get for a nonexistent request
# ------------------------------------------------------------------------------
request_fake=$(uuidgen)
JSON=$(cat <<EOF
{
    "id": "$request_fake"
}
EOF
)

bash $TEMPLATES/action-member-empty "$SET" "pri" "request" "get" "$JSON" 1
