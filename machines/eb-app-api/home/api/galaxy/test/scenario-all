#!/usr/bin/bash
set -e

# ------------------------------------------------------------------------------
# environment
# ------------------------------------------------------------------------------
BASEDIR=$(dirname $0)
source $BASEDIR/config
source $BASEDIR/scripts/common

for i in $(seq 1 10); do
    declare ID${i}=$i
done

# ------------------------------------------------------------------------------
# kratos
# ------------------------------------------------------------------------------
for i in $(seq 1 10); do
    bash $BASEDIR/scripts/kratos/registration $i
    bash $BASEDIR/scripts/kratos/login $i
done

# ------------------------------------------------------------------------------
# adm
# ------------------------------------------------------------------------------
bash $BASEDIR/scripts/adm/hello

# ------------------------------------------------------------------------------
# pri
# ------------------------------------------------------------------------------
bash $BASEDIR/scripts/pri/hello $ID1

# profile
bash $BASEDIR/scripts/pri/profile/add-invalid $ID1
bash $BASEDIR/scripts/pri/profile/add-new $ID1 1
bash $BASEDIR/scripts/pri/profile/get-exist $ID1 1
bash $BASEDIR/scripts/pri/profile/get-none $ID1
bash $BASEDIR/scripts/pri/profile/get-default $ID1 1
bash $BASEDIR/scripts/pri/profile/add-exist $ID1 1

bash $BASEDIR/scripts/pri/profile/update-exist $ID1 1
bash $BASEDIR/scripts/pri/profile/update-none $ID1
bash $BASEDIR/scripts/pri/profile/update-invalid $ID1 1
bash $BASEDIR/scripts/pri/profile/del-exist $ID1 1
bash $BASEDIR/scripts/pri/profile/del-none $ID1

for i in $(seq 1 10); do
    bash $BASEDIR/scripts/pri/profile/add-new $ID1 $i
done

bash $BASEDIR/scripts/pri/profile/set-default-exist $ID1 1
bash $BASEDIR/scripts/pri/profile/set-default-none $ID1
bash $BASEDIR/scripts/pri/profile/list-nolimit-nooffset $ID1
bash $BASEDIR/scripts/pri/profile/list-limit $ID1
bash $BASEDIR/scripts/pri/profile/list-limit-offset $ID1
bash $BASEDIR/scripts/pri/profile/list-huge-offset $ID1
bash $BASEDIR/scripts/pri/profile/list-invalid $ID1

bash $BASEDIR/scripts/pri/profile/add-new $ID2 1
bash $BASEDIR/scripts/pri/profile/get-default $ID2 1

# domain
bash $BASEDIR/scripts/pri/domain/add-invalid $ID1
bash $BASEDIR/scripts/pri/domain/add-new $ID1 1
bash $BASEDIR/scripts/pri/domain/get-exist $ID1 1
bash $BASEDIR/scripts/pri/domain/get-none $ID1
bash $BASEDIR/scripts/pri/domain/add-exist $ID1 1

bash $BASEDIR/scripts/pri/domain/update-exist $ID1 1
bash $BASEDIR/scripts/pri/domain/update-none $ID1
bash $BASEDIR/scripts/pri/domain/update-invalid $ID1 1
bash $BASEDIR/scripts/pri/domain/disable-exist $ID1 1
bash $BASEDIR/scripts/pri/domain/disable-none $ID1
bash $BASEDIR/scripts/pri/domain/enable-exist $ID1 1
bash $BASEDIR/scripts/pri/domain/enable-none $ID1
bash $BASEDIR/scripts/pri/domain/del-exist $ID1 1
bash $BASEDIR/scripts/pri/domain/del-none $ID1

for i in $(seq 1 10); do
    bash $BASEDIR/scripts/pri/domain/add-new $ID1 $i
done

bash $BASEDIR/scripts/pri/domain/list-nolimit-nooffset $ID1
bash $BASEDIR/scripts/pri/domain/list-limit $ID1
bash $BASEDIR/scripts/pri/domain/list-limit-offset $ID1
bash $BASEDIR/scripts/pri/domain/list-huge-offset $ID1
bash $BASEDIR/scripts/pri/domain/list-invalid $ID1

# domain-invite
bash $BASEDIR/scripts/pri/domain-invite/add-invalid $ID1
bash $BASEDIR/scripts/pri/domain-invite/add-new $ID1 1 1
bash $BASEDIR/scripts/pri/domain-invite/get-exist $ID1 1
bash $BASEDIR/scripts/pri/domain-invite/get-none $ID1
bash $BASEDIR/scripts/pri/domain-invite/get-bycode-exist $ID1 1
bash $BASEDIR/scripts/pri/domain-invite/get-bycode-none $ID1

bash $BASEDIR/scripts/pri/domain-invite/disable-exist $ID1 1
bash $BASEDIR/scripts/pri/domain-invite/disable-none $ID1
bash $BASEDIR/scripts/pri/domain-invite/enable-exist $ID1 1
bash $BASEDIR/scripts/pri/domain-invite/enable-none $ID1
bash $BASEDIR/scripts/pri/domain-invite/del-exist $ID1 1
bash $BASEDIR/scripts/pri/domain-invite/del-none $ID1

for i in $(seq 1 20); do
    (( d = (i - 1) % 10 + 1 ))
    bash $BASEDIR/scripts/pri/domain-invite/add-new $ID1 $i $d
done

bash $BASEDIR/scripts/pri/domain-invite/list-bydomain-nolimit-nooffset $ID1 1
bash $BASEDIR/scripts/pri/domain-invite/list-bydomain-limit $ID1 1
bash $BASEDIR/scripts/pri/domain-invite/list-bydomain-huge-offset $ID1 1
bash $BASEDIR/scripts/pri/domain-invite/list-bydomain-invalid $ID1 1

# domain-partnership
bash $BASEDIR/scripts/pri/domain-partnership/add-bycode-invalid $ID2
bash $BASEDIR/scripts/pri/domain-invite/get-exist $ID1 1
bash $BASEDIR/scripts/pri/domain-partnership/add-bycode-new $ID2 1 $ID1 1
bash $BASEDIR/scripts/pri/domain-partnership/get-exist $ID2 1
bash $BASEDIR/scripts/pri/domain-partnership/get-none $ID2
bash $BASEDIR/scripts/pri/domain-partnership/add-bycode-exist $ID2 1

bash $BASEDIR/scripts/pri/domain-partnership/del-exist $ID2 1
bash $BASEDIR/scripts/pri/domain-partnership/del-none $ID2

bash $BASEDIR/scripts/pri/domain-invite/enable-exist $ID1 1
for i in $(seq 1 10); do
    bash $BASEDIR/scripts/pri/domain-invite/get-exist $ID1 $i
    bash $BASEDIR/scripts/pri/domain-partnership/add-bycode-new $ID2 $i $ID1 $i
done

# domain-partner
bash $BASEDIR/scripts/pri/domain-partner/get-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/domain-partner/get-none $ID1

bash $BASEDIR/scripts/pri/domain-partner/disable-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/domain-partner/disable-none $ID1
bash $BASEDIR/scripts/pri/domain-partner/enable-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/domain-partner/enable-none $ID1

bash $BASEDIR/scripts/pri/domain-partner/list-bydomain-nolimit-nooffset $ID1 1
bash $BASEDIR/scripts/pri/domain-partner/list-bydomain-limit $ID1 1
bash $BASEDIR/scripts/pri/domain-partner/list-bydomain-huge-offset $ID1 1
bash $BASEDIR/scripts/pri/domain-partner/list-bydomain-invalid $ID1 1

bash $BASEDIR/scripts/pri/domain-partner/del-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/domain-partner/del-none $ID1

for i in $(seq 3 10); do
    ID="$i"
    (( j = 8 + i ))
    bash $BASEDIR/scripts/pri/domain-invite/get-exist $ID1 $j
    bash $BASEDIR/scripts/pri/domain-partnership/add-bycode-new $ID 1 $ID1 $j
done

# contact
bash $BASEDIR/scripts/pri/contact/list-nolimit-nooffset $ID1
bash $BASEDIR/scripts/pri/contact/list-limit $ID1
bash $BASEDIR/scripts/pri/contact/list-limit-offset $ID1
bash $BASEDIR/scripts/pri/contact/list-huge-offset $ID1
bash $BASEDIR/scripts/pri/contact/list-invalid $ID1

bash $BASEDIR/scripts/pri/contact/get-exist $ID1 1
bash $BASEDIR/scripts/pri/contact/get-none $ID1

bash $BASEDIR/scripts/pri/contact/list-bydomain-nolimit-nooffset $ID1
bash $BASEDIR/scripts/pri/contact/list-bydomain-limit $ID1
bash $BASEDIR/scripts/pri/contact/list-bydomain-limit-offset $ID1
bash $BASEDIR/scripts/pri/contact/list-bydomain-huge-offset $ID1
bash $BASEDIR/scripts/pri/contact/list-bydomain-invalid $ID1

# domain-partner-candidate
#bash $BASEDIR/scripts/pri/domain-partner-candidate/add-invalid $ID1
exit

# room
bash $BASEDIR/scripts/pri/room/add-invalid $ID1
bash $BASEDIR/scripts/pri/room/add-new $ID1 1 1
bash $BASEDIR/scripts/pri/room/get-exist $ID1 1
bash $BASEDIR/scripts/pri/room/get-none $ID1
bash $BASEDIR/scripts/pri/room/add-exist $ID1 1

bash $BASEDIR/scripts/pri/room/update-exist $ID1 1 1
bash $BASEDIR/scripts/pri/room/update-none $ID1
bash $BASEDIR/scripts/pri/room/update-invalid $ID1
bash $BASEDIR/scripts/pri/room/disable-exist $ID1 1
bash $BASEDIR/scripts/pri/room/disable-none $ID1
bash $BASEDIR/scripts/pri/room/enable-exist $ID1 1
bash $BASEDIR/scripts/pri/room/enable-none $ID1
bash $BASEDIR/scripts/pri/room/del-exist $ID1 1
bash $BASEDIR/scripts/pri/room/del-none $ID1

for i in $(seq 1 20); do
    (( d = (i - 1) % 10 + 1 ))
    bash $BASEDIR/scripts/pri/room/add-new $ID1 $i $d
    bash $BASEDIR/scripts/pri/room/add-ephemeral $ID1 $i $d
done

bash $BASEDIR/scripts/pri/room/list-nolimit-nooffset $ID1
bash $BASEDIR/scripts/pri/room/list-limit $ID1
bash $BASEDIR/scripts/pri/room/list-limit-offset $ID1
bash $BASEDIR/scripts/pri/room/list-huge-offset $ID1
bash $BASEDIR/scripts/pri/room/list-invalid $ID1

bash $BASEDIR/scripts/pri/room/get-link-exist $ID1 1
bash $BASEDIR/scripts/pri/room/get-link-none $ID1

# room-invite
bash $BASEDIR/scripts/pri/room-invite/add-invalid $ID1
bash $BASEDIR/scripts/pri/room-invite/add-new $ID1 1 1
bash $BASEDIR/scripts/pri/room-invite/get-exist $ID1 1
bash $BASEDIR/scripts/pri/room-invite/get-none $ID1
bash $BASEDIR/scripts/pri/room-invite/get-bycode-exist $ID1 1
bash $BASEDIR/scripts/pri/room-invite/get-bycode-none $ID1

bash $BASEDIR/scripts/pri/room-invite/disable-exist $ID1 1
bash $BASEDIR/scripts/pri/room-invite/disable-none $ID1
bash $BASEDIR/scripts/pri/room-invite/enable-exist $ID1 1
bash $BASEDIR/scripts/pri/room-invite/enable-none $ID1
bash $BASEDIR/scripts/pri/room-invite/del-exist $ID1 1
bash $BASEDIR/scripts/pri/room-invite/del-none $ID1

for i in $(seq 1 20); do
    (( r = (i - 1) % 10 + 1 ))
    bash $BASEDIR/scripts/pri/room-invite/add-new $ID1 $i $r
done

bash $BASEDIR/scripts/pri/room-invite/list-byroom-nolimit-nooffset $ID1 1
bash $BASEDIR/scripts/pri/room-invite/list-byroom-limit $ID1 1
bash $BASEDIR/scripts/pri/room-invite/list-byroom-huge-offset $ID1 1
bash $BASEDIR/scripts/pri/room-invite/list-byroom-invalid $ID1 1

# room-partnership
bash $BASEDIR/scripts/pri/room-partnership/add-bycode-invalid $ID2
bash $BASEDIR/scripts/pri/room-invite/get-exist $ID1 1
bash $BASEDIR/scripts/pri/room-partnership/add-bycode-new $ID2 1 $ID1 1
bash $BASEDIR/scripts/pri/room-partnership/get-exist $ID2 1
bash $BASEDIR/scripts/pri/room-partnership/get-none $ID2
bash $BASEDIR/scripts/pri/room-partnership/add-bycode-exist $ID2 1 $ID1 1

bash $BASEDIR/scripts/pri/room-partnership/del-exist $ID2 1
bash $BASEDIR/scripts/pri/room-partnership/del-none $ID2

bash $BASEDIR/scripts/pri/room-invite/enable-exist $ID1 1
for i in $(seq 1 10); do
  bash $BASEDIR/scripts/pri/room-invite/get-exist $ID1 $i
  bash $BASEDIR/scripts/pri/room-partnership/add-bycode-new $ID2 $i $ID1 $i
done

# room-partner
bash $BASEDIR/scripts/pri/room-partner/get-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/room-partner/get-none $ID1

bash $BASEDIR/scripts/pri/room-partner/disable-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/room-partner/disable-none $ID1
bash $BASEDIR/scripts/pri/room-partner/enable-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/room-partner/enable-none $ID1

bash $BASEDIR/scripts/pri/room-partner/list-byroom-nolimit-nooffset $ID1 1
bash $BASEDIR/scripts/pri/room-partner/list-byroom-limit $ID1 1
bash $BASEDIR/scripts/pri/room-partner/list-byroom-huge-offset $ID1 1
bash $BASEDIR/scripts/pri/room-partner/list-byroom-invalid $ID1 1

bash $BASEDIR/scripts/pri/room-partner/del-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/room-partner/del-none $ID1

# meeting
bash $BASEDIR/scripts/pri/meeting/add-invalid $ID1
bash $BASEDIR/scripts/pri/meeting/add-new-permanent $ID1 1 1
bash $BASEDIR/scripts/pri/meeting/get-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting/get-none $ID1

bash $BASEDIR/scripts/pri/meeting/update-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting/update-none $ID1
bash $BASEDIR/scripts/pri/meeting/update-invalid $ID1
bash $BASEDIR/scripts/pri/meeting/disable-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting/disable-none $ID1
bash $BASEDIR/scripts/pri/meeting/enable-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting/enable-none $ID1
bash $BASEDIR/scripts/pri/meeting/del-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting/del-none $ID1

for i in $(seq 1 20); do
    (( r = (i - 1) % 10 + 1 ))
    bash $BASEDIR/scripts/pri/meeting/add-new-permanent $ID1 $i $r
    bash $BASEDIR/scripts/pri/meeting/add-new-scheduled $ID1 $i $r
done

bash $BASEDIR/scripts/pri/meeting/list-nolimit-nooffset $ID1
bash $BASEDIR/scripts/pri/meeting/list-limit $ID1
bash $BASEDIR/scripts/pri/meeting/list-limit-offset $ID1
bash $BASEDIR/scripts/pri/meeting/list-huge-offset $ID1
bash $BASEDIR/scripts/pri/meeting/list-invalid $ID1

bash $BASEDIR/scripts/pri/meeting/get-link-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting/get-link-none $ID1

# meeting-invite
bash $BASEDIR/scripts/pri/meeting-invite/add-invalid $ID1
bash $BASEDIR/scripts/pri/meeting-invite/add-new-host-audience-of-permanent \
    $ID1 1 1
bash $BASEDIR/scripts/pri/meeting-invite/add-new-guest-audience-of-permanent \
    $ID1 1 1
bash $BASEDIR/scripts/pri/meeting-invite/add-new-host-member-of-permanent \
    $ID1 1 1
bash $BASEDIR/scripts/pri/meeting-invite/add-new-guest-member-of-permanent \
    $ID1 1 1
bash $BASEDIR/scripts/pri/meeting-invite/add-new-host-audience-of-scheduled \
    $ID1 1 1
bash $BASEDIR/scripts/pri/meeting-invite/add-new-guest-audience-of-scheduled \
    $ID1 1 1
bash $BASEDIR/scripts/pri/meeting-invite/add-new-host-member-of-scheduled \
    $ID1 1 1
bash $BASEDIR/scripts/pri/meeting-invite/add-new-guest-member-of-scheduled \
    $ID1 1 1
bash $BASEDIR/scripts/pri/meeting-invite/get-exist-host-member-of-permanent \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/get-exist-host-member-of-scheduled \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/get-exist-host-audience-of-permanent \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/get-exist-host-audience-of-scheduled \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/get-none $ID1
bash $BASEDIR/scripts/pri/meeting-invite/get-bycode-exist-host-member-of-permanent \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/get-bycode-exist-host-member-of-scheduled \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/get-bycode-none $ID1

bash $BASEDIR/scripts/pri/meeting-invite/disable-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/disable-none $ID1
bash $BASEDIR/scripts/pri/meeting-invite/enable-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/enable-none $ID1
bash $BASEDIR/scripts/pri/meeting-invite/del-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/del-none $ID1

for i in $(seq 1 20); do
    (( m = (i - 1) % 10 + 1 ))
    bash $BASEDIR/scripts/pri/meeting-invite/add-new-host-audience-of-permanent \
        $ID1 $i $m
    bash $BASEDIR/scripts/pri/meeting-invite/add-new-guest-audience-of-permanent \
        $ID1 $i $m
    bash $BASEDIR/scripts/pri/meeting-invite/add-new-host-member-of-permanent \
        $ID1 $i $m
    bash $BASEDIR/scripts/pri/meeting-invite/add-new-guest-member-of-permanent \
        $ID1 $i $m
    bash $BASEDIR/scripts/pri/meeting-invite/add-new-host-audience-of-scheduled \
        $ID1 $i $m
    bash $BASEDIR/scripts/pri/meeting-invite/add-new-guest-audience-of-scheduled \
        $ID1 $i $m
    bash $BASEDIR/scripts/pri/meeting-invite/add-new-host-member-of-scheduled \
        $ID1 $i $m
    bash $BASEDIR/scripts/pri/meeting-invite/add-new-guest-member-of-scheduled \
        $ID1 $i $m
done

bash $BASEDIR/scripts/pri/meeting-invite/list-bymeeting-permanent-nolimit-nooffset \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/list-bymeeting-permanent-limit $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/list-bymeeting-permanent-huge-offset \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/list-bymeeting-permanent-invalid \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/list-bymeeting-scheduled-nolimit-nooffset \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/list-bymeeting-scheduled-limit $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/list-bymeeting-scheduled-huge-offset \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-invite/list-bymeeting-scheduled-invalid $ID1 1

# meeting-membership
bash $BASEDIR/scripts/pri/meeting-membership/add-bycode-invalid $ID2
bash $BASEDIR/scripts/pri/meeting-invite/get-exist-host-member-of-permanent \
    $ID1 1
bash $BASEDIR/scripts/pri/meeting-membership/add-bycode-new-host-member-of-permanent \
    $ID2 1 $ID1 1
bash $BASEDIR/scripts/pri/meeting-membership/get-exist-host-member-of-permanent \
    $ID2 1
bash $BASEDIR/scripts/pri/meeting-membership/get-none $ID2
bash $BASEDIR/scripts/pri/meeting-membership/add-bycode-exist $ID2 1

bash $BASEDIR/scripts/pri/meeting-membership/update-exist $ID2 1
bash $BASEDIR/scripts/pri/meeting-membership/update-none $ID2
bash $BASEDIR/scripts/pri/meeting-membership/del-exist $ID2 1
bash $BASEDIR/scripts/pri/meeting-membership/del-none $ID2

bash $BASEDIR/scripts/pri/meeting-invite/enable-exist $ID1 1
for i in $(seq 1 10); do
    bash $BASEDIR/scripts/pri/meeting-invite/get-exist-host-member-of-permanent \
        $ID1 $i
    bash $BASEDIR/scripts/pri/meeting-membership/add-bycode-new-host-member-of-permanent \
        $ID2 $i $ID1 $i
    bash $BASEDIR/scripts/pri/meeting-invite/get-exist-host-member-of-scheduled \
        $ID1 $i
    bash $BASEDIR/scripts/pri/meeting-membership/add-bycode-new-host-member-of-scheduled \
        $ID2 $i $ID1 $i
done

# meeting-member
bash $BASEDIR/scripts/pri/meeting-member/get-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/meeting-member/get-none $ID1

bash $BASEDIR/scripts/pri/meeting-member/list-bymeeting-nolimit-nooffset $ID1 1
bash $BASEDIR/scripts/pri/meeting-member/list-bymeeting-limit $ID1 1
bash $BASEDIR/scripts/pri/meeting-member/list-bymeeting-huge-offset $ID1 1
bash $BASEDIR/scripts/pri/meeting-member/list-bymeeting-invalid $ID1 1

bash $BASEDIR/scripts/pri/meeting-member/disable-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/meeting-member/disable-none $ID1
bash $BASEDIR/scripts/pri/meeting-member/enable-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/meeting-member/enable-none $ID1
bash $BASEDIR/scripts/pri/meeting-member/set-guest-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/meeting-member/set-guest-none $ID1
bash $BASEDIR/scripts/pri/meeting-member/set-host-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/meeting-member/set-host-none $ID1
bash $BASEDIR/scripts/pri/meeting-member/del-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/meeting-member/del-none $ID1

# meeting-request
bash $BASEDIR/scripts/pri/meeting-request/add-invalid $ID2
bash $BASEDIR/scripts/pri/meeting-request/add-new $ID2 1 $ID1 1
bash $BASEDIR/scripts/pri/meeting-request/get-exist $ID2 1
bash $BASEDIR/scripts/pri/meeting-request/get-none $ID2
bash $BASEDIR/scripts/pri/meeting-request/add-exist-request $ID2 1
bash $BASEDIR/scripts/pri/meeting-request/add-exist-membership $ID2 1

bash $BASEDIR/scripts/pri/meeting-request/update-exist $ID2 1
bash $BASEDIR/scripts/pri/meeting-request/update-none $ID2
bash $BASEDIR/scripts/pri/meeting-request/update-invalid $ID2
bash $BASEDIR/scripts/pri/meeting-request/del-exist $ID2 1
bash $BASEDIR/scripts/pri/meeting-request/del-none $ID2

bash $BASEDIR/scripts/pri/meeting-request/drop-none $ID1
bash $BASEDIR/scripts/pri/meeting-request/add-new $ID2 1 $ID1 1
bash $BASEDIR/scripts/pri/meeting-request/drop-exist $ID1 1 $ID2 1

bash $BASEDIR/scripts/pri/meeting-request/reject-none $ID1
bash $BASEDIR/scripts/pri/meeting-request/add-new $ID2 1 $ID1 1
bash $BASEDIR/scripts/pri/meeting-request/reject-exist $ID1 1 $ID2 1
bash $BASEDIR/scripts/pri/meeting-request/drop-exist $ID1 1 $ID2 1

bash $BASEDIR/scripts/pri/meeting-request/accept-none $ID1
bash $BASEDIR/scripts/pri/meeting-request/add-new $ID2 1 $ID1 1
bash $BASEDIR/scripts/pri/meeting-request/accept-exist $ID1 1 $ID2 1

for i in $(seq 1 10); do
    bash $BASEDIR/scripts/pri/meeting/add-new-permanent $ID1 $i
    bash $BASEDIR/scripts/pri/meeting-request/add-new $ID2 $i $ID1 $i
done

bash $BASEDIR/scripts/pri/meeting-request/list-nolimit-nooffset $ID2
bash $BASEDIR/scripts/pri/meeting-request/list-limit $ID2
bash $BASEDIR/scripts/pri/meeting-request/list-limit-offset $ID2
bash $BASEDIR/scripts/pri/meeting-request/list-huge-offset $ID2
bash $BASEDIR/scripts/pri/meeting-request/list-invalid $ID2

# meeting-schedule
bash $BASEDIR/scripts/pri/meeting-schedule/add-invalid $ID1
bash $BASEDIR/scripts/pri/meeting-schedule/add-new $ID1
bash $BASEDIR/scripts/pri/meeting-schedule/get-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting-schedule/get-none $ID1

bash $BASEDIR/scripts/pri/meeting-schedule/update-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting-schedule/update-none $ID1
bash $BASEDIR/scripts/pri/meeting-schedule/update-invalid $ID1
bash $BASEDIR/scripts/pri/meeting-schedule/del-exist $ID1 1
bash $BASEDIR/scripts/pri/meeting-schedule/del-none $ID1

for i in $(seq 1 10); do
